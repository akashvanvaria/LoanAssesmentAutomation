      <processControlElements>
        <inputCondition id="InputCondition">
          <flowsInto>
            <nextElementRef id="Enter_the_Application_Data" />
          </flowsInto>
        </inputCondition>
        <task id="Enter_the_Application_Data">
          <name>Enter the Application Data</name>
          <flowsInto>
            <nextElementRef id="Generate_Application_ID" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <completedMappings>
            <mapping>
              <expression query="&lt;PropertyInformation&gt;{/Enter_the_Application_Data/PropertyInformation/*}&lt;/PropertyInformation&gt;" />
              <mapsTo>PropertyInformation</mapsTo>
            </mapping>
            <mapping>
