            </mapping>
          </completedMappings>
          <resourcing>
            <offer initiator="system">
              <distributionSet>
                <initialSet>
                  <participant>PA-3df76a7e-23f5-4fdf-ab3c-c67250b7529b</participant>
                  <participant>PA-d2377be0-2bbe-4577-9acf-92247f0bd88e</participant>
                  <role>RO-e90a577f-162e-4d2e-b7f2-b71d39e5aceb</role>
